apiVersion: v1
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/pki/ca.pem
    server: https://{{ apiserver_vip }}:{{ apiserver_vip_port }}
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: system:node:{{ ansible_fqdn }}
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:node:{{ ansible_fqdn }}
  user:
    client-certificate: /etc/kubernetes/pki/kubelet.pem
    client-key: /etc/kubernetes/pki/kubelet-key.pem
